<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    new_alarm_clcok_item_llyt

    <style name="new_alarm_clcok_item_llyt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:background">@drawable/shape_circle_dialog</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="new_alarm_clcok_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">54dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="new_alarm_clcok_item_type">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">19dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black_text_normal_common</item>
    </style>

    <style name="new_alarm_clcok_item_describe">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/darkgray</item>
    </style>

    <style name="new_alarm_clcok_repeat_days_tog_btn">
        <item name="android:layout_width">38dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:background">@drawable/toggle_btn_repeat_week</item>
        <item name="android:checked">false</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@drawable/text_color_toggle_btn_circle</item>
    </style>

    <style name="new_alarm_clock_repeat_days_llyt">
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

</resources>